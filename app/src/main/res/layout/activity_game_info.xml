<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ryan.places.GameInfo">

    <TextView
        android:id="@+id/sport"
        android:layout_width="0dp"
        android:layout_height="34dp"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="53dp"
        app:layout_constraintBottom_toBottomOf="@+id/city"
        android:layout_marginEnd="53dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="42dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="34dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="53dp"
        android:layout_marginRight="53dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/compLevel"
        android:layout_width="0dp"
        android:layout_height="34dp"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/city"
        app:layout_constraintTop_toBottomOf="@+id/city"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/city"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:id="@+id/date"
        android:layout_width="0dp"
        android:layout_height="34dp"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/compLevel"
        app:layout_constraintTop_toBottomOf="@+id/compLevel"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/compLevel"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:id="@+id/time"
        android:layout_width="0dp"
        android:layout_height="34dp"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/date"
        app:layout_constraintTop_toBottomOf="@+id/date"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/date"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:id="@+id/postalAddress"
        android:layout_width="0dp"
        android:layout_height="34dp"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/time"
        app:layout_constraintTop_toBottomOf="@+id/time"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/time"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:id="@+id/owner"
        android:layout_width="0dp"
        android:layout_height="34dp"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/postalAddress"
        app:layout_constraintTop_toBottomOf="@+id/postalAddress"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/postalAddress"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:id="@+id/city"
        android:layout_width="0dp"
        android:layout_height="34dp"
        android:layout_marginEnd="53dp"
        android:layout_marginLeft="53dp"
        android:layout_marginRight="53dp"
        android:layout_marginStart="53dp"
        android:layout_marginTop="76dp"
        android:text="TextView"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Button
        android:id="@+id/signup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SIGN UP AS PLAYER"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="53dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="119dp"
        android:layout_marginRight="53dp" />

    <TextView
        android:id="@+id/or"
        android:layout_width="86dp"
        android:layout_height="40dp"
        android:text="OR"
        android:textSize="24sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="19dp"
        app:layout_constraintRight_toRightOf="@+id/ref_button"
        android:layout_marginTop="63dp"
        android:layout_marginBottom="64dp"
        app:layout_constraintTop_toTopOf="@+id/signup"
        android:layout_marginRight="19dp" />

    <Button
        android:id="@+id/ref_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="REFEREE THIS GAME"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="53dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/or"
        android:layout_marginRight="53dp" />

    <TextView
        android:id="@+id/signed_view"
        android:layout_width="0dp"
        android:layout_height="17dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintRight_toRightOf="@+id/refs_view"
        android:layout_marginTop="11dp"
        app:layout_constraintTop_toBottomOf="@+id/owner"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="10dp"
        app:layout_constraintLeft_toLeftOf="@+id/refs_view" />

    <TextView
        android:id="@+id/refs_view"
        android:layout_width="191dp"
        android:layout_height="18dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="53dp"
        android:layout_marginTop="10dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="71dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/signed_view"
        android:layout_marginLeft="53dp" />

    <Button
        android:id="@+id/leave_butt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="-23dp"
        android:layout_marginLeft="144dp"
        android:layout_marginStart="8dp"
        android:text="Leave Game"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@+id/refs_view"
        app:layout_constraintLeft_toLeftOf="@+id/refs_view"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <Button
        android:id="@+id/attend_butt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="-23dp"
        android:layout_marginLeft="144dp"
        android:layout_marginStart="8dp"
        android:text="Attendees"
        app:layout_constraintTop_toBottomOf="@+id/refs_view"
        app:layout_constraintLeft_toLeftOf="@+id/refs_view"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1" />
</android.support.constraint.ConstraintLayout>
